<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<joined-subclass extends="dao.Vehicle" name="dao.Bicycle" table="BICYCLE">
		<key column="BYCICLE_ID" />
		<property name="durchmesser" type="double" column="DIAMETER"/>
		<property name="name" type="string" column="BICYCLE_NAME"/>
	</joined-subclass>

</hibernate-mapping>